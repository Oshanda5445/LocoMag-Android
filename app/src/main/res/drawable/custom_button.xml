<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <gradient android:startColor= "#e5a98b" android:centerColor="#441e0b" android:endColor="#e5a98b" android:angle="90"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
        <gradient android:startColor= "#726f6e" android:centerColor="#2d241f" android:endColor="#726f6e" android:angle="90"/>
        <corners android:radius="5dp"/>
        </shape>
    </item>
</selector>